
<div class="all">
  <div class="more">
<p id="notice"><%= notice %></p>

<img src="http://1000logos.net/wp-content/uploads/2016/10/Batman-logo.png" height="100px" width="200px">
<h1>Bat Messenger</h1>
<table>
  <thead>
    <tr>
      <th>Bat Subject</th>
      <th>Bat Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogs.each do |blog| %>
      <tr>
        <td><%= blog.title %></td>
        <td><%= blog.body %></td>
        <td><%= link_to 'View', blog %></td>
        <td><%= link_to 'Edit', edit_blog_path(blog) %></td>
        <td><%= link_to 'Delete', blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bat Message', new_blog_path %>
</div>
</div>
